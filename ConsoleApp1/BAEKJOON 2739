using System;

namespace Bronze5
{
    class Program
    {
        //구구단
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());
            
            for (int i = 1; i < 10; i++)
            {
                string re = n + " * " + i + " = " + (n * i);
                Console.WriteLine(re);
            }
        }
    }
}
