using System;
using System.Text;

namespace Bronze5
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());
            //문자열 변환 성능 향상
            StringBuilder sb = new StringBuilder();
            
            for (int i = 1; i < n + 1; i++)
            {
                sb.AppendLine(i.ToString());
            }
            Console.WriteLine(sb);
            
        }
    }
}
