using System;

namespace Bronze3
{
    class Program
    {
        static void Main()
        {
            string N = Console.ReadLine();
            int Num = int.Parse(N);

            if (Num % 4 == 0 && Num % 100 != 0 || Num % 400 == 0)
            {
                Console.WriteLine("1");
            }
            else
            {
                Console.WriteLine("0");
            }

        }
    }
}
